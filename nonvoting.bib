@incollection{fatke_wollen_2015,
	title = {Wollen sie nicht, können sie nicht, oder werden sie nicht gefragt? {Nichtwählertypen} in der {Schweiz}},
	isbn = {978-3-03810-098-0},
	shorttitle = {Wollen sie nicht, können sie nicht, oder werden sie nicht gefragt?},
	abstract = {Dieser Beitrag untersucht die Zusammensetzung der Nichtwählerschaft bei den Schweizer Parlamentswahlen 2011. Der weit verbreiteten Auffassung, bei Nichtwählern handelte es sich um eine einheitliche, monolithische Gruppe, werden zunächst theoretisch aus dem Civic Voluntarism Model hergeleitete Kategorien gegenübergestellt. Ferner wird auf Grundlage der Selects-Daten die Nichtwählerschaft auch empirisch faktor- und clusteranalytisch untersucht. Dabei treten sechs distinkte Typen von Nichtwählern zutage, die sich anhand verschiedener Merkmale und Motive als zufrieden desinteressierte, politisch verdrossene, abstimmende, unkonventionell partizipierende, sozial isolierte oder inkompetente Nichtwähler charakterisieren lassen. Es zeigt sich also, dass das oftmals simplifizierte Bild von Nichtwählern der bunt gemischten Zusammensetzung nicht gerecht wird und vielmehr eine differenziertere Betrachtung verlangt. Dies dient uns im Folgenden als Anlass, politische und demokratietheoretische Implikationen elektoraler Abstinenz zu diskutieren.},
	author = {Fatke, Matthias and Freitag, Markus},
	month = jan,
	year = {2015},
	pages = {95--119},
}

